@model IEnumerable<desarrolloPrivadoEmanuel.Models.Empresa>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoEmpresa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoEmpresa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodEmpresa">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodEmpresa">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodEmpresa">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
